{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "import/extensions": 0,
    "no-use-before-define": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  }
}
